<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane tabClosingPolicy="UNAVAILABLE" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
    <tabs>
        <Tab closable="false" text="%tab.students.title">
            <content>
                <fx:include source="tab-students.fxml"/>
            </content>
        </Tab>
        <Tab closable="false" text="%tab.courses.title">
            <content>
                <fx:include source="tab-courses.fxml"/>
            </content>
        </Tab>
        <Tab closable="false" text="%tab.registrations.title">
            <content>
                <fx:include source="tab-registrations.fxml"/>
            </content>
        </Tab>
    </tabs>
</TabPane>
